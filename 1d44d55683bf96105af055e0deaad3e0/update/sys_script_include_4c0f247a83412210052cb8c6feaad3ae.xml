        <script><![CDATA[var AiDevices = Class.create();
AiDevices.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	getAiDetails: function(){

		var device = this.getParameter('sysparm_device');
		var gr = new GlideRecord('x_1619838_techwith_ai_devices');
		gr.addQuery('sys_id', device);
		gr.query();
		if(gr.next()){
			var x = {

				price: gr.getValue('u_price'),
				model: gr.getValue('model_number'),
				warrenty: gr.getValue('warranty_expiration'),
				description: gr.getValue('short_description')



			};
			return JSON.stringify(x);
		}

		

	},

    type: 'AiDevices'
});]]></script>
